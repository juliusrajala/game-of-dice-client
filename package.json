{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "For kickstarting simple apps on the fly",
  "main": "index.js",
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "start": "npm run build-static && webpack-dev-server --config webpack.dev.js --mode development",
    "clean": "rm -rf dist",
    "build-static": "shx rm -rf dist/ && shx mkdir -p dist/ && cp -rf public/assets/ dist/assets/ && cp public/reset.css dist/",
    "serve": "npm run build && http-server ./dist -p 8000",
    "build": "npm run build-static && webpack --config webpack.prod.js --optimize-minimize",
    "prettify": "prettier --write src/"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Julius Rajala <juliusrajala@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/react": "~16.9.32",
    "@types/react-dom": "~16.9.6",
    "@types/react-router-dom": "~5.1.4",
    "@types/socket.io-client": "~1.4.32",
    "@types/styled-components": "~5.0.1",
    "compression-webpack-plugin": "~3.1.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "~4.2.0",
    "http-server": "~0.12.1",
    "prettier": "~2.0.2",
    "redux-devtools-extension": "^2.13.8",
    "shx": "^0.2.2",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "~2.3.5",
    "ts-loader": "4.3.0",
    "typescript": "^2.9.2",
    "webpack": "~4.42.1",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "~3.10.3",
    "webpack-merge": "~4.2.2"
  },
  "dependencies": {
    "date-fns": "~2.11.1",
    "history": "~4.10.1",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-icons": "~3.9.0",
    "react-router-dom": "~5.1.2",
    "socket.io-client": "~2.3.0",
    "styled-components": "^5.0.1",
    "styled-reset": "~4.1.3",
    "ulid": "~2.3.0"
  }
}
