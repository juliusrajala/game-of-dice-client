{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "For kickstarting simple apps on the fly",
  "main": "index.js",
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "start": "npm run build-static && webpack-dev-server --mode development",
    "clean": "rm -rf dist",
    "build-static": "shx mkdir -p dist/ && cp -r public/* dist/",
    "serve": "npm run build && http-server ./dist -p 8000",
    "build": "npm run build-static && webpack --mode production",
    "prettify": "prettier --write src/"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Julius Rajala <juliusrajala@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/react": "~16.9.32",
    "@types/react-dom": "~16.9.6",
    "file-loader": "1.1.11",
    "http-server": "~0.12.1",
    "prettier": "~2.0.2",
    "redux-devtools-extension": "^2.13.8",
    "shx": "^0.2.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "4.3.0",
    "typescript": "^2.9.2",
    "webpack": "~4.42.1",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.10.3"
  },
  "dependencies": {
    "@types/styled-components": "~5.0.1",
    "date-fns": "~2.11.1",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-icons": "~3.9.0",
    "styled-components": "^5.0.1",
    "ulid": "~2.3.0"
  }
}
